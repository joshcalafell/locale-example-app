{
  "name": "locale-example-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-i18n:fr": "ng build --output-path=dist/fr --aot -prod --bh /fr/ --i18n-file=src/locale/messages.fr.xlf --i18n-format=xlf --locale=fr",
    "build-i18n:ch": "ng build --output-path=dist/ch --aot -prod --bh /ch/ --i18n-file=src/locale/messages.ch.xlf --i18n-format=xlf --locale=ch",
    "build-i18n:en": "ng build --output-path=dist/en --aot -prod --bh /en/ --i18n-file=src/locale/messages.en.xlf --i18n-format=xlf --locale=en",
    "build-i18n": "npm run build-i18n:en && npm run build-i18n:ch && npm run build-i18n:fr",
    "serve-i18n:en": "ng serve --aot -prod --bh /en/ --port 5280 --i18n-file=src/locale/messages.en.xlf --i18n-format=xlf --locale=en",
    "serve-i18n:fr": "ng serve --aot -prod --bh /fr/ --port 5281 --i18n-file=src/locale/messages.fr.xlf --i18n-format=xlf --locale=fr",
    "serve-i18n:ch": "ng serve --aot -prod --bh /ch/ --port 5282 --i18n-file=src/locale/messages.ch.xlf --i18n-format=xlf --locale=zh",
    "serve-i18n": "npm run serve-i18n:fr && npm run serve-i18n:en && npm run serve-i18n:ch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular-devkit/core": "0.0.28",
    "@angular/cli": "1.6.3",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  }
}
